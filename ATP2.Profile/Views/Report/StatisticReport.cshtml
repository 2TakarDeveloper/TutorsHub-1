
@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StatisticReport</title>
</head>
<body>
<div>
    <fieldset>
        <table width="80%" cellpadding="0" cellspacing="0">
            <tr>
                <td>
                    <b>USER | STATISTICS</b>
                </td>
                <td align="right">
                    <input type="submit" value="Print"/>
                </td>
            </tr>
        </table>
    </fieldset>
    <br/>
    <table width="80%" cellspacing="0" border="1" cellpadding="5">
        <tr>
            <th align="left">ROLE</th>
            <th align="center" width="110">NO OF USERS</th>
        </tr>
        <tr>
            <td>Admin</td>
            <td align="center">@Model.Admin</td>        
        </tr>
        <tr>
            <td>Executive</td>
            <td align="center">@Model.Executive</td>        
        </tr>
        <tr>
            <td>Tutor</td>
            <td align="center">@Model.Tutor</td>        
        </tr>
        <tr>
            <td align="right"><b>Total</b></td>
            <td align="center"><b>@Model.Total</b></td>        
        </tr>
    </table>
</div>
</body>
</html>

                        <a href="/Student/Report" type="button" class="btn btn-primary" style="margin:8px 0px">Report</a>
                        <a href="/Student/ChangePassword" type="button" class="btn btn-primary" style="margin:8px 0px">Change Passsword</a>
                    </div>
                </div>
                <div class="col-sm-10">
                    @RenderBody()

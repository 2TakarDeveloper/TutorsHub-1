@model Entity.UserModels.Student
@{
    ViewBag.Title = "StudentProfile";
    Layout = "~/Views/Shared/StudentLayout.cshtml";
}
<div style="background-color:white; border:1px solid green;margin:0px 50px; padding:30px">
    <h3>Welcome to Profile</h3>
    <hr>
    <p>Name : @Model.Name</p>
    <hr/>
    <p>Email : @Model.Email</p>
    <hr />
    <p>Phone : @Model.Mobilenumber</p>
    <hr />
    <p>Last Active : 1 day ago</p>
    <hr />
    <p>Account Activated : 1 year ago</p>
    <hr />
    <div class="form-group" id="submit">
        <button type="button" class="btn btn-info" onclick="myFunction()">Edit Profile</button>
        <script>
            function myFunction() {
                window.location.href="/student/EditProfile";
            }
        </script>
    </div>
</div>